{
  "name": "hw-frontend-migration-vuejs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "yarn serve & yarn start",
    "dev:win": "start yarn serve & start yarn start",
    "serve": "parcel ./index.html -d ./dev --no-source-maps --no-autoinstall --no-cache",
    "build": "rm -rf ./dist && parcel build ./index.html -d ./dist --no-source-maps --no-cache",
    "start": "node app.js"
  },
  "repository": "git@github.com:BoYuanSu/vuejs-frontend-migration.git",
  "author": "BYSu <mayday15221@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.1.0",
    "promise-polyfill": "^8.2.0",
    "vee-validate": "^3.4.5",
    "vue": "^2.6.12",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "parcel-bundler": "1.12.3",
    "sass": "^1.32.8"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.dev.js"
  }
}
